{
  "name": "typey-boi",
  "productName": "Typey Boi",
  "version": "0.0.1",
  "description": "Practice typing by reading a book.",
  "repository": "https://github.com/Meebuhs/typey-boi",
  "homepage": "./",
  "main": "public/electron.js",
  "author": "Alex Sharpe",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "parcel public/index.html --out-dir build",
    "build": "parcel build public/index.html --out-dir build --public-url ./",
    "lint": "eslint src/**/*.{ts,tsx} && stylelint src/**/*.scss --fix",
    "precommit": "yarn prettier && yarn lint",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "yarn build && electron-builder -w"
  },
  "dependencies": {
    "components": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typescript": "^3.9.6"
  },
  "build": {
    "appId": "com.meebuhs.typey-boi",
    "win": {
      "target": "portable"
    },
    "files": [
      "build",
      "public"
    ]
  }
}
