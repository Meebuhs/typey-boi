{
  "name": "booktyper",
  "version": "0.0.1",
  "description": "Practice typing by reading a book.",
  "repository": "https://github.com/Meebuhs/booktyper",
  "author": "Alex Sharpe",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "lint": "eslint src/**/*.{ts,tsx} && stylelint src/**/*.scss --fix",
    "precommit": "yarn prettier && yarn lint",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "dist": "mkdir -p ./dist && cd build && zip -r ../dist/${npm_package_version}.zip *"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typescript": "^3.9.6"
  }
}
